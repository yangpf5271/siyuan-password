  "scripts": {
    "install": "cd app && pnpm install",
    "apply-patches": "cd app && pnpm run apply-patches",
    "dev": "cd app && pnpm run dev",
    "start": "cd app && pnpm run start",
    "build": "cd app && pnpm run build",
    "tsc": "cd app && pnpm run tsc",
    "lint": "cd app && pnpm run lint",
    "dist-darwin": "cd app && pnpm run dist-darwin",
    "dist-darwin-arm64": "cd app && pnpm run dist-darwin-arm64",
    "dist-win": "cd app && pnpm run dist-win",
    "dist-linux": "cd app && pnpm run dist-linux",
    "dist-linux-arm64": "cd app && pnpm run dist-linux-arm64",
    "dist-appx": "cd app && pnpm run dist-appx",
    "sync-upstream": "bash scripts/sync-upstream.sh"
